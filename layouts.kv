################################### Custom Class Stuff ##############################

################################### All Windows #####################################

WindowManager:
    MainWindow:

#################################### Main Window Layout #############################
<MainWindow>
    name: "main_window"

    MDGridLayout:
        cols: 1
        
        canvas.before:
            Color:
                rgb: (.18, .18, .18)
            Rectangle:
                pos: self.pos
                size: self.size

        

            
        TabbedPanel:
            size_hint: .5, .5
            pos_hint: {'center_x': .5, 'center_y': .5}
            do_default_tab: False

            TabbedPanelItem:
                text: 'Guide'
                
                MDGridLayout:
                    cols: 1
            
                    ScrollView:

                        MDLabel:
                            id: journal_guide
                            text: "Please select the date"
                            size_hint_y: None
                            text_size: self.width, None
                            height: self.texture_size[1]


                    MDFloatingActionButton:
                        id: note_button
                        icon: "book-cross"
                        elevation: 0
                        on_release:
                            nav_drawer.set_state("open")

            TabbedPanelItem:
                text: 'Journal'

                MDTextField:
                    id: journal_questions_text
                    size_hint_y: 1
                    hint_text: "Question Answers: "
                    multiline: True
                    mode: "fill"
                    icon_right: "book-edit"
                    text: ""
                    on_touch_up: root.save_note()

        MDBottomAppBar:

            MDTopAppBar:
                type: "bottom"
                icon: "information-variant"
                mode: "end"
                on_action_button: root.about()
                Image: 
                    source: "logo_nobg.png"
                    allow_stretch: True
                    keep_ratio: False
                    size_hint: None, None
                    width: 200
                    height: 70

    MDNavigationDrawer:
        id: nav_drawer

        orientation: "vertical"
        
        MDList:
            OneLineListItem:
                text: "The 2023 Reading Plan"

        MDScrollView:
            
            MDList:
                
                TwoLineIconListItem:
                    
                    text: "JAN 1"
                    secondary_text: "Passage: Matt 1-2"
                    on_press: root.display_journal("JAN 1", "Matt 1-2")
                    on_release: nav_drawer.set_state("closed")


